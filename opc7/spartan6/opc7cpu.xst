set -tmpdir "tmp"
set -xsthdpdir "xst"
run
-ifn opc7cpu.prj
-p xc6slx9-csg225-3
-ifmt mixed
-ofn opc7cpu
-ofmt NGC
-top opc7cpu
-opt_mode Area
-opt_level 2
-iuc NO
-lso opc7cpu.lso
-keep_hierarchy No
-netlist_hierarchy as_optimized
-rtlview Yes
-hierarchy_separator /
-bus_delimiter <>
-case maintain
-fsm_extract YES
-fsm_encoding Auto
-safe_implementation No
-resource_sharing YES
-iobuf YES
-equivalent_register_removal YES
